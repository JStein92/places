<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Home page</title>
  </head>
  <body>
    <h1>My Cities Visited</h1>
    @if (Model!=null)
    {
      @foreach(var place in Model)
      {
        <ul>
        <li>City: <a href="/@place.getId()">@place.GetCity()</a></li>
        <li>Stayed: @place.GetDays()</li>
        
        </ul>
      }
    }

    <a href="/new-place">Submit new place</a>


  </body>
</html>
